{
  "name": "eldiablo",
  "version": "1.0.0",
  "description": "Quick proof of concept to do course exports.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js"
  },
  "author": "Andrew Gremlich",
  "license": "MIT",
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": false
      }
    },
    "plugins": [],
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "devDependencies": {
    "babel-preset-latest": "^6.16.0"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "prompt": "^1.0.0",
    "zombie": "^5.0.5"
  }
}
